{
   "name": "@funtech-inc/mekuri",
   "version": "2.0.4",
   "description": "Page transition animation with Next.js",
   "main": "./build/main.cjs",
   "module": "./build/main.js",
   "types": "./types/index.d.ts",
   "exports": {
      ".": {
         "import": "./build/main.js",
         "require": "./build/main.cjs"
      },
      "./register": {
         "import": {
            "types": "./types/utils/register.d.ts",
            "default": "./build/register.js"
         },
         "require": {
            "types": "./types/utils/register.d.ts",
            "default": "./build/register.cjs"
         }
      }
   },
   "type": "module",
   "scripts": {
      "type": "tsc",
      "build": "tsc && vite build",
      "clean": "rimraf build && rimraf types"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/FunTechInc/mekuri"
   },
   "keywords": [
      "react",
      "next"
   ],
   "author": "HASHIMOTO Takuma",
   "license": "MIT",
   "files": [
      "build",
      "types"
   ],
   "peerDependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
   },
   "devDependencies": {
      "typescript": "^5.1.3",
      "vite": "^4.3.9"
   }
}
